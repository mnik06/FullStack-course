{
  "name": "softonix-guidelines-vite",
  "version": "2.1.0",
  "type": "module",
  "scripts": {
    "dev-only": "vite --host",
    "dev": "run-s dev-only",
    "build-only": "vite build",
    "build": "run-s type-check build-only",
    "preview": "vite preview",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "openapi-generate": "npx openapi-typescript https://fakerestapi.azurewebsites.net/swagger/v1/swagger.json -o ./src/api/types/schema.d.ts",
    "postinstall": "run-p openapi-generate"
  },
  "dependencies": {
    "@vueuse/core": "^12.3.0",
    "axios": "^1.7.9",
    "element-plus": "^2.9.2",
    "pinia": "^2.3.0",
    "portal-vue": "^3.0.0",
    "vue": "^3.5.13",
    "vue-i18n": "^11.0.1",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.12.1",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.10.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.32.0",
    "npm-run-all2": "^7.0.2",
    "pathe": "^1.1.2",
    "postcss": "^8.4.49",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass": "^1.83.1",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.2",
    "unplugin-auto-import": "^0.19.0",
    "unplugin-icons": "^0.22.0",
    "unplugin-vue-components": "^0.28.0",
    "vite": "^6.0.7",
    "vite-plugin-vue-devtools": "^7.7.0",
    "vue-tsc": "^2.2.0"
  }
}
